<Window x:Class="MSSA_FINAL_PROJECT_WORKING.SavePresetWindow"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Save Preset" Height="200" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <TextBox x:Name="PresetNameTextBox" Margin="0,0,0,10"/>
            <TextBlock Text="Preset Name" Margin="0,0,0,5"/>
            <TextBox x:Name="PresetDescriptionTextBox" Margin="0,0,0,10"/>
            <TextBlock Text="Preset Description" Margin="0,0,0,5"/>
            <Button Content="Save" Click="SaveButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
