<Window x:Class="MSSA_FINAL_PROJECT_WORKING.ReadSystemDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MSSA_FINAL_PROJECT_WORKING"
        mc:Ignorable="d"
        Title="Read System Data from CSV" Height="200" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <Label Content="File Path"/>
            <TextBox x:Name="FilePathTextBox"/>
            <Button Content="Read" Click="ReadButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
