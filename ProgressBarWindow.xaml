<Window x:Class="MSSA_FINAL_PROJECT_WORKING.ProgressBarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MSSA_FINAL_PROJECT_WORKING"
        mc:Ignorable="d"
        Title="Progress" Height="100" Width="300">
    <Grid>
        <ProgressBar x:Name="ProgressBar" IsIndeterminate="True" 
                     HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="10"/>
    </Grid>
</Window>
